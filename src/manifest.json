{
   "manifest_version": 2,
   "name": "CC",
   "version": "0.1",

   "content_scripts": [
      {
         "matches": [ "<all_urls>" ],
         "js": ["cc_fg.js"]
      }
   ],
   "background": {
      "scripts": ["cc_bg.js"]
   },

   "browser_action": {
      "default_icon": "img/icon_0.png",
      "default_popup": "cc_popup.html"
   },
   "permissions" : [
      "contentSettings",
      "contextMenus",
      "tabs",
      "http://*/*",
      "https://*/*"
   ]
}
