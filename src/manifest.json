{
   "manifest_version": 2,
   "name": "CC",
   "version": "0.1",

   "background": {
      "scripts": ["cc_bg.js"]
   },

   "browser_action": {
      "default_icon": "img/icon_0.png",
      "default_popup": "cc_popup.html"
   },
   "permissions" : [
      "contentSettings",
      "contextMenus",
      "tabs",
      "http://*/*",
      "https://*/*"
   ]
}
